<% if_p('nats.machines') do |_| %>
message_bus_servers:
<% p('nats.machines').each do |ip| %>
    - host: <%= ip %>:<%= p('nats.port') %>
      user: <%= p('nats.user') %>
      password: <%= p('nats.password') %>
<% end %>
external_host: proxy-<%= index %>.<%= p('external_host') %>
external_ip: <%= spec.networks.send(p('network_name')).ip %>
port: <%= p('proxy.api_port') %>
<% end %>
